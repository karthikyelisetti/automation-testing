package learningMethods;

public class ArgumentedMethods {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		m1(37);
		System.out.println("----------------");
		m2("LearningMethods");
		System.out.println("----------------");
		m3(false);
		System.out.println("----------------");

	}
	
	//When we declare a variable in the methods it is know as argumented method
	public static void m1(int age) {
		System.out.println("Age = " + age);
		System.out.println("Executing m1 ()");
	}
	
	public static void m2(String name) {
		//m1(32);
		System.out.println("Name = " + name);
		System.out.println("Executing m2 ()");
	}
	
	public static void m3(boolean status) {
		//m2("LearningMethods");
		System.out.println("Status = "+ status);
		System.out.println("Executing m3 ()");
	}

}
